{
  "name": "apolloserver",
  "version": "1.0.0",
  "description": "Minimal set of code to write a GraphQL server with Apollo graphql-tools",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./server.js --exec babel-node -e js,graphql",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yogakurniawan/spend-graphql.git"
  },
  "keywords": [
    "Node.js",
    "Javascript",
    "GraphQL",
    "Express",
    "Apollo",
    "Mongo DB",
    "Mongoose"
  ],
  "author": "Yoga Kurniawan <yogaygk@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^1.1.2",
    "body-parser": "^1.15.2",
    "eslint": "^4.6.1",
    "express": "^4.14.0",
    "graphql": "^0.7.2",
    "graphql-tools": "^0.7.2",
    "mongoose": "^4.6.4",
    "request": "^2.75.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "nodemon": "^1.11.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true,
      "node": true,
      "es6": true
    }
  }
}
